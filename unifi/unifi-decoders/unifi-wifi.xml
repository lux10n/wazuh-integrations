<decoder name="custom-wifi-auth">
  <prematch>hostapd</prematch>
</decoder>
<decoder name="custom-wifi-auth-header">
  <parent>custom-wifi-auth</parent>
  <regex>(\.+):\s(\w+)\s*(\d+)\s*(\d*:\d*:\d*)\s*(\.+)\s*(\S+):\s*hostapd\p(\d*)\p</regex>
  <order>log_source,month,day,hour,host_dir,device_id,pid</order>
</decoder>
<decoder name="custom-wifi-auth-header">
  <parent>custom-wifi-auth</parent>
  <regex>hostapd[\d*]:\s(\S+):\s(\S+)\s(\.+)\s(\.+):\s*(\w*)</regex>
  <order>net_interface,station,mac,protocol_standar,status</order>
</decoder>