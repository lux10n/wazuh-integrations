<!-- Modify it at your will. -->
<decoder name="custom-syn-flood">
  <prematch>SYN flooding</prematch>
</decoder>
<decoder name="custom-syn-flood-header">
  <parent>custom-syn-flood</parent>
  <regex>(\.+):\s*(\w+)\s*(\d+)\s*(\d+:\d+:\d+)\s*(\.+)\s*(\.+):\s*kernel:\s*\p(\d+\p*\d+)\p TCP: request_sock_TCP: (\.+)</regex>
  <order>log_source,month,day,hour,host_dir,device_id,pid,message</order>
</decoder>
